const int a_ascii_code = 97;

class Square {
string terrain = "";
int meadows;
bool is_starting_square = false;
Resource@[] resources;
string[] additional_attributes;
int x,y;

Square(int x, int y, int meadows) {
this.x = x;
this.y = y;
this.meadows = meadows;
}

string coordinates() {
string letter = ascii_to_character(a_ascii_code + this.x);
return letter + (this.y + 1);
}
}